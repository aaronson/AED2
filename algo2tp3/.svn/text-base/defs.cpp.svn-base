#include "defs.h"

bool operator==(const actividad a, const actividad b){
	return a.orden == b.orden && a.tarea == b.tarea;
}

bool operator==(const activpriorizada a, const activpriorizada b){
	return a.act == b.act && a.prioridad == b.prioridad;
}

